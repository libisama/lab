#include <iostream>

using namespace std;

int main()
{
    setlocale(LC_ALL, "Russian");
    int a, b, c;
    cout << "Введите три стороны треугольника (целые числа)." << endl;
    cin >> a >> b >> c;

    if ((a<b+c) && (b<a+c) && (c<a+b)) { cout << "Высказывание 'Существует треугольник со сторонами a, b, c' истинно." << endl;
}
    else { cout << "Высказывание 'Существует треугольник со сторонами a, b, c' ложно." << endl; }

    return 0;
}
