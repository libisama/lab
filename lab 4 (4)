#include <iostream>

using namespace std; 

int main()
{
    setlocale(LC_ALL, "Russian");
    float a, b, sum, ras, pr, ch;
    cout << "Введите два ненулевых числа." << endl;
    cin >> a >> b;
    sum = sqrt(a) + sqrt(b);
    ras = sqrt(a) - sqrt(b);
    pr = sqrt(a) * sqrt(b);
    ch = sqrt(a) / sqrt(b);
    cout << "Сумма квадратов чисел равна " << sum << "." << endl;
    cout << "Разность квадратов чисел равна " << ras << "." << endl;
    cout << "Произведение квадратов чисел равно " << pr << "." << endl;
    cout << "Частное квадратов чисел равно " << ch << "." << endl;

    return 0;
}
