#include <iostream>

using namespace std;

int main()
{
    setlocale(LC_ALL, "Russian");
    int n, n1, n2, a, b, c, s;
    cout << "Введите три целых положительных числа." << endl;
    cin >> a >> b >> c;

    n1 = a / c;
    n2 = b / c;
    n = n1 * n2;
    s = a * b - n * c * c;

    cout << "Количество квадратов - " << n << "." << endl;
    cout << "Оставшаяся площадь - " << s << "." << endl;
    return 0;
}
