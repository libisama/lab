#include <iostream>

using namespace std;

int main()
{
    setlocale(LC_ALL, "Russian");
    int a, b;
    cout << "Введите два целых числа." << endl;
    cin >> a >> b;

    if (a == b) { a = 0; b = 0; }
    else {
        if (a > b) { b = a; }
        else { a = b; }
    }

    cout << "A = " << a << "." << endl;
    cout << "B = " << b << "." << endl;
    return 0;
}
