#include <iostream>

using namespace std;

int main()
{
    setlocale(LC_ALL, "Russian");
    int a, b;
    cout << "Введите два целых числа." << endl;
    cin >> a >> b;

    if ((a>2) && (b<=3)) { cout << "Высказывание 'Справедливы неравенства A > 2 и B <= 3' истинно." << endl;
}
    else { cout << "Высказывание 'Справедливы неравенства A > 2 и B <= 3' ложно." << endl; }

    return 0;
}
