#include <iostream>

using namespace std;
int Sign (float X)
{
    if (X < 0) X=-1;
    if (X == 0) X=0;
    if (X > 0) X=1;
    return X;
}

int main()
{
    {
        setlocale(LC_ALL, "Russian");
        float A, B;
            cout << "Введите два вещественных числа." << endl;
            cin >> A >> B;
            cout << Sign(A)+Sign(B) << endl;
    }

    return 0;
}
